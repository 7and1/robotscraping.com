# AI Provider (openai, anthropic, openrouter)
AI_PROVIDER=openrouter

# OpenRouter Configuration (Recommended - Free tier available)
# Get keys at: https://openrouter.ai/keys
OPENROUTER_API_KEY_1=your-openrouter-key-1-here
OPENROUTER_API_KEY_2=your-openrouter-key-2-here
OPENROUTER_API_KEY_3=your-openrouter-key-3-here
OPENROUTER_MODEL=meta-llama/llama-3.3-70b-instruct:free
OPENROUTER_FALLBACK_MODEL_1=deepseek/deepseek-r1:free
OPENROUTER_FALLBACK_MODEL_2=google/gemini-3-flash-preview

# Legacy: OpenAI
OPENAI_API_KEY=sk-xxx
OPENAI_MODEL=gpt-4o-mini

# Legacy: Anthropic
ANTHROPIC_API_KEY=sk-ant-xxx
ANTHROPIC_MODEL=claude-3-haiku-20240307

# Worker Settings
MAX_CONTENT_CHARS=20000
BROWSER_TIMEOUT_MS=15000
CORS_ORIGIN=https://robotscraping.com
DEFAULT_SCREENSHOT=false
STORE_CONTENT=true
ALLOW_ANON=false
CACHE_ENABLED=true
CACHE_TTL_MS=900000

# Proxy Grid (Optional)
PROXY_GRID_ENABLED=false
PROXY_GRID_BASE_URL=http://google.savedimage.com
PROXY_GRID_ALLOWLIST=
PROXY_GRID_FORCE=false

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS=60
RATE_LIMIT_WINDOW_MS=60000
MAX_REQUEST_SIZE_MB=10
ENABLE_RATE_LIMIT_HEADERS=true

# Security: WEBHOOK_SECRET must be set via wrangler secret put
# Never commit actual secrets to git
